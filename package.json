{
  "name": "node-bootstrap3-template",
  "version": "0.0.1",
  "description": "a template for express, node.js, mysql and react",
  "main": "app.js",
  "scripts": {
    "test": "npm run lint && npm run test-once",
    "test-once": "karma start",
    "test-watch": "karma start --no-single-run --browser Chrome",
    "lint": "eslint app spec --ext .js,.jsx",
    "start": "node app",
    "build-dev": "webpack --debug --devtool=inline-source-map --watch --display-reasons --display-error-details --progress --colors --config config/webpack.config.js",
    "build": "webpack -p --display-reasons --display-error-details --progress --colors --config config/webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:mjhea0/node-bootstrap3-template.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "template",
    "twitter",
    "bootstrap",
    "bootstrap3"
  ],
  "author": "Michael Herman <michael@mherman.org>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.4.5",
    "babel-loader": "^6.2.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "config": "^1.19.0",
    "css-loader": "^0.23.1",
    "backbone": "^1.3.2",
    "chai": "^3.4.1",
    "express": "~3.3.4",
    "file-loader": "^0.8.5",
    "history": "^1.17.0",
    "jade": "^0.34.1",
    "mocha": "^2.3.4",
    "mysql": "^2.10.2",
    "mysql-model": "0.0.2",
    "node-sass": "^3.4.2",
    "passport": "^0.3.2",
    "passport-facebook": "^2.0.0",
    "passport-github2": "^0.1.9",
    "passport-google-oauth2": "^0.1.6",
    "passport-instagram": "^1.0.0",
    "passport-twitter": "^1.0.3",
    "react": "^0.14.7",
    "react-checkbox-group": "^0.3.1",
    "react-dom": "^0.14.7",
    "react-router": "^1.0.3",
    "react-timers": "^5.0.0",
    "reactable": "^0.12.2",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "should": "^7.1.1",
    "underscore": "^1.8.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.13"
  },
  "devDependencies": {
    "babel-jest": "^6.0.1",
    "chai": "^3.5.0",
    "chai-react": "^3.0.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "jquery": "^2.2.0",
    "karma": "^0.13.19",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.2",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "lodash": "^4.5.0",
    "mocha": "^2.4.5",
    "npm-check-updates": "^2.5.7",
    "phantomjs-prebuilt": "^2.1.4",
    "react-addons-test-utils": "^0.14.7",
    "requirejs": "^2.1.22",
    "rquery": "^3.1.1",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0"
  }
